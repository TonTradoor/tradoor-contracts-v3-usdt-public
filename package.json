{
    "name": "tradoor-contracts-v3",
    "version": "0.0.1",
    "scripts": {
        "start": "blueprint run",
        "build": "blueprint build --all",
        "buildPool": "blueprint build Pool",
        "test": "jest --verbose --maxWorkers=1",
        "test-build": "yarn build && yarn test",
        "run": "blueprint run --mnemonic",
        "runlocal": "NODE_ENV=localnet blueprint run --mnemonic",
        "rundev": "NODE_ENV=devnet blueprint run --mnemonic",
        "runtest": "NODE_ENV=testnet blueprint run --mnemonic",
        "runmain": "NODE_ENV=mainnet blueprint run",
        "deploylocal": "sh scripts/deploy.sh localnet",
        "deploydev": "sh scripts/deploy.sh devnet",
        "deploytest": "sh scripts/deploy.sh testnet",
        "deploymain": "sh scripts/deployMain.sh"
    },
    "devDependencies": {
        "@orbs-network/ton-access": "^2.3.3",
        "@ton/blueprint": "^0.17.0",
        "@ton/core": "^0.56.0",
        "@ton/crypto": "^3.2.0",
        "@ton/sandbox": "^0.19.0",
        "@ton/test-utils": "^0.4.2",
        "@ton/ton": "^13.11.1",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.7",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "jest": "^29.7.0",
        "prettier": "^3.2.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@aws-crypto/sha256-js": "^5.2.0",
        "decimal.js": "^10.4.3"
    }
}
